program Suma;
var
  a    : Integer;
  b    : Integer;
  c    : Integer;
begin
  writeln('Programa para sumar dos numeros enteros');
  writeln('Dame el Primer Numero');
  Readln(a);
  writeln('Dame el Segundo Numero');
  Readln(b);
  c:=a+b;
  writeln('La suma es ',c);
  readln;
end.		
